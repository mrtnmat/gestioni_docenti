        android:layout_marginEnd="8dp"
        android:layout_weight="1"
        android:background="#B5B4B3"
        android:gravity="center"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/pwd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout"
        app:smsp_alwaysShowFloatingLabel="true"
        app:smsp_arrowColor="@color/black"
        app:smsp_arrowPaddingBottom="5sp"
        app:smsp_arrowSize="30dp"
        app:smsp_enableFloatingLabel="false"
        app:smsp_floatingLabelColor="@color/black"
        app:smsp_floatingLabelText="Prof."
        app:smsp_hint="Seleziona il Docente"
        app:smsp_hintColor="@color/black"
        app:smsp_hintSize="25dp"
        app:smsp_itemColor="#000000"
        app:smsp_itemListBackgroundColor="@color/purple_500"
        app:smsp_itemListColor="#D92CF6"
        app:smsp_itemListHintBackgroundColor="@color/teal_700"
        app:smsp_itemListHintColor="#FFFFFF"
        app:smsp_itemSize="16dp"
        app:smsp_selectedItemListColor="#FF0000" />

    <FrameLayout
        android:id="@+id/contentGramsci"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:contextClickable="true"
        android:longClickable="true"
        android:paddingLeft="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:id="@+id/gramsciColor"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            android:backgroundTint="#656060"
            android:gravity="center"
            android:longClickable="true"
            android:text="Gramsci"
            android:textStyle="bold" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/contentVilla"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:addStatesFromChildren="false"
        android:background="#FFFFFF"
        android:clickable="false"
        android:clipToPadding="false"
        android:contextClickable="true"
        android:longClickable="true"
        android:pointerIcon="arrow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/villaColor"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            android:backgroundTint="#656060"
            android:gravity="center"
            android:longClickable="true"
            android:text="Villa"
            android:textStyle="bold" />

    </FrameLayout>

    <EditText
        android:id="@+id/pwd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="88dp"
        android:ems="10"
        android:hint="Inserisci Pwd per modificare i dati"
        android:inputType="textPassword"
        android:textColorHighlight="#58EA30"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/aggiungiPresenza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prova" />


